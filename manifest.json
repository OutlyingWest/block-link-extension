{
    "name": "link blocker",
    "description": "close tabs with blocked links",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "16":"icons/16x16.png",
        "32":"icons/32x32.png",
        "48":"icons/48x48.png",
        "128":"icons/128x128.png"
    },
    "action": {
        "default_popup":"popup.html"
    },
    "permissions": ["tabs", "url"],
    "background": {
        "service_worker": "background.js"
    }
}